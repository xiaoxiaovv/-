def sdf = new SimpleDateFormat('yyyy-MM-dd HH:mm:ss')
String startTime = "1970-01-01 00:00:00"
Date startDate = sdf.parse(startTime)
List<Site> allSites = siteService.getAllSites();
def noDataMap=[:]//key
def hasDataMap=[:]
def returnMap=[:]
for (int i = 0; i < allSites.size(); i++) {
    Site site = allSites.get(i)
    String isIncluded="否"
    String websiteDomain = site.websiteDomain
    if (urlsMap.containsKey(websiteDomain)){
        isIncluded="是"
    }
    if (hasDataMap.containsKey(websiteDomain)){
        continue;
    }
    List newsList = siteRepo.getSitesNewsFromEs([site], 0, startDate, new Date(), 0, false, 1, "", 1, 1)
    if (newsList && newsList.size()>0){
        hasDataMap.put(websiteDomain,"1")
        continue;
    }else {
        if (noDataMap.containsKey(websiteDomain)){
            int count =returnMap.get(websiteDomain).get("count")
            def obj=[:]
            obj.put("count",count+1)
            obj.put("isIncluded",isIncluded)
            returnMap.put(websiteDomain,obj)
        }else {
            def obj=[:]
            obj.put("count",1)
            obj.put("isIncluded",isIncluded)
            returnMap.put(websiteDomain,obj)
        }
    }
}
println(JSON.toJSONString(returnMap, SerializerFeature.WriteMapNullValue))