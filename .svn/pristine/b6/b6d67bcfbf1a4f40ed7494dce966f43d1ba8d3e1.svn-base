@media (max-width: 1220px) {
    .editor-cont-center .editor-page .editor-page-tools { left: calc(50% + 565px); }
    .bjj-main.min-bar .editor-cont-center .editor-page .editor-page-tools { left: calc(50% + 480px); }
    .editor-cont-center .editor-page .edui-container .edui-toolbar.position-toolbar { position: fixed; left: calc(50% - 125px); top: 50px; }
}
@media (max-width: 992px) {
    .editor-sidebar-right { display: none; }
    .editor-cont-center.mobile-editor-cont-center { width: calc(100% - 300px); }
}
@media (min-width: 1550px) {
    .editor-sidebar-right { width: 600px; }
    .editor-cont-center.mobile-editor-cont-center { width: calc( 100% - 900px)!important; }
    .editor-cont-center .editor-page .edui-container .edui-toolbar.position-toolbar.hasDetail-toolbar.hasDetail-position-toolbar { position: fixed; left: inherit !important; right: calc( (100% - 1550px)/2) !important; top: 50px; }
}
/*新增文稿引导页面*/
.out-manuscripts { position:fixed;top:0; left:0; right:0; bottom:0; background:rgba(61,61,61,.8); z-index: 1040; }
.out-manuscripts .in-manuscripts { position: absolute; }
.out-manuscripts .in-manuscripts .manuscripts { width: 50px; background: #fff; line-height: 34px; border-radius: 4px; color: #2494f2; padding: 0 10px; }
.out-manuscripts .in-manuscripts .topic { width: 80px; }
.out-manuscripts .in-manuscripts .manuscripts-tips { width: 210px; height: 130px; line-height: 20px; border-radius: 4px; background: #fff; border: 1px solid #fff; top: 48px; padding: 10px; font-size: 14px; color: #000; position: absolute; }
.out-manuscripts .in-manuscripts .manuscripts-tips:after { content: ' '; width: 10px; height: 10px; border: 1px solid #fff; position: absolute; top: -5px; left: 15px; transform: rotate(45deg); display: inline-block; border-right-width: 0; border-bottom-width: 0; background: #fff;}
.out-manuscripts .in-manuscripts .manuscripts-tips p { height: 15px; margin: 0; }
.out-manuscripts .in-manuscripts .manuscripts-tips p .fa-times { float: right; color: #c4c4c4; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title { height: 65px; padding: 0 5px; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title .content { font-size: 12px; color: #333; line-height: 1.5rem; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title .step { line-height: 20px; text-align: center; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title .select-topic { line-height: 10px; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title .step .circle { display: inline-block; width: 6px; height: 6px; border-radius: 50%; background: #c4c4c4; }
.out-manuscripts .in-manuscripts .manuscripts-tips .title .step .circle.main { background: #999; }
.out-manuscripts .in-manuscripts .manuscripts-tips .manuscripts-btn { height: 25px; }
.out-manuscripts .in-manuscripts .manuscripts-tips .manuscripts-btn button { width: 68px; line-height: 10px; height: 25px; padding: 0 5px; font-size: 12px; }
.out-manuscripts .in-manuscripts .manuscripts-tips .manuscripts-btn .prev-btn { background: #999; border-color: #fff; }
.out-manuscripts .tips { top: 59px; left: 62px; }
.out-manuscripts .data { top: 59px; left: 131px; display: none; }
.out-manuscripts .selected-top { right: 180px; top: 75px; display: none; }

/*模板*/
.template .modal-body .container-fluid { padding-left: 12px; padding-right: 0px; }
.template .modal-title span { display: inline-block; width: 80px; text-align: center; cursor: pointer; }
.template .modal-title span.active { color: #2494f2; }
.template .modal-body { padding-top: 20px; }
.template .modal-body .form-horizontal .form-group { margin-left: 15px; margin-bottom: 18px; }
.template .modal-body .form-horizontal .form-group label.leftTitle { width: 80px; margin-top: 5px; }
.template .modal-body .form-horizontal .form-group input.templateName { width: 145px; padding-left: 10px; }
.template .modal-body .form-horizontal .form-group div.custom-text { width: 170px; height: 35px; float: left; margin-right: 27px; }
.template .modal-body .form-horizontal .form-group div.custom-text input { width: 145px; }
.template .modal-body .form-horizontal .form-group div.custom-text input.error { border-color: #ff0030; }
.template .modal-body .form-horizontal .form-group div.custom-text span { display: inline-block; color: #999999; margin: 6px 0 0 8px; }
.template .modal-body .form-horizontal .form-group .rightTitle { margin-top: 7px; }
.template .modal-body .form-horizontal .form-group .rightTitle .textIndent{ margin-right: 33px;}
.template .modal-body .form-horizontal .form-group .rightTitle label.pull-left { margin-right: 15px; }
.template input[type='radio'] { width: 14px; height: 14px; float: left; margin: 3px 7px 0 0px; position: relative; background: transparent; border: 0; outline: none; appearance: none; -webkit-appearance: none; -moz-appearance:none;}
.template input[type='radio']:before { width: 14px; height: 14px; content: ""; position: absolute; border: 1px solid #dddddd; border-radius: 16px; }
.template input[type='radio']:checked:before { border: 1px solid #2494f2; border-radius: 16px; background: #ffffff; }
.template input[type='radio']:checked:after { width: 12px; height: 12px; content: ""; position: absolute; top: 1px; left: 1px; border: 2px solid #fff; border-radius: 100%; background: #2494f2;}
.template .modal-footer button{ width: 88px; height: 34px; }
.template .modal-footer button.applyTemplate { color: #999999; }
.template .modal-footer button.applyTemplate:hover { background: #ffffff; color: #999999; border: 1px solid #cccccc; cursor: inherit; }
.template .toggle-button,.template .one-toggle-button,.template .two-toggle-button{ display: none; }
.template .button-label{position: relative; display: inline-block; width: 50px; height: 20px; background-color: #dddddd; border-radius: 30px; overflow: hidden; }
.template .circle{ position: absolute; top: 2px; left: 32px; width: 16px; height: 16px; border-radius: 50%; background-color: #ffffff; }
.template .button-label .text { line-height: 20px; font-size: 12px; text-shadow: 0 0 2px #dddddd; }
.template .button-label .on { color: #ffffff; display: none; text-indent: 6px; }
.template .button-label .off { color: #ffffff; display: inline-block; text-indent: 21px;}
.template .button-label .circle{ left: 0px; transition: all 0.3s; }
.template .toggle-button:checked + label.button-label.offIndent .circle, .template .one-toggle-button:checked + label.button-label.offIndent .circle,.template .two-toggle-button:checked + label.button-label.offIndent .circle{ left: 32px; }
.template .toggle-button:checked + label.button-label.offIndent .on, .template .one-toggle-button:checked + label.button-label.offIndent .on, .template .two-toggle-button:checked + label.button-label.offIndent .on{ display: inline-block; }
.template .toggle-button:checked + label.button-label.offIndent .off, .template .one-toggle-button:checked + label.button-label.offIndent .off ,.template .two-toggle-button:checked + label.button-label.offIndent .off{ display: none; }
.template .toggle-button:checked + label.button-label.offIndent, .template .one-toggle-button:checked + label.button-label.offIndent, .template .two-toggle-button:checked + label.button-label.offIndent{ background-color: #5dcb20; }

