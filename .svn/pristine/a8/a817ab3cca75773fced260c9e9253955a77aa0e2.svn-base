.evaluate-header{position: absolute;background: #fef7c3; width: 100%;height: 40px;line-height: 40px;text-align: center;z-index: 9999;}
.evaluate-header>span{color:#4351b3;}
.evaluate-body { padding: 48px 40px 80px 40px; background: #fff; }
.evaluate-body .evaluate-every-box { padding-bottom: 22px; border-bottom: 1px solid #eceff8; margin-bottom: 40px; }
.evaluate-body .evaluate-every-box .module-name { font-size: 15px; color: #191919; }
.evaluate-body .evaluate-every-box select { width: 112px; height: 30px; float: left; margin-left: 20px; }
.evaluate-body .evaluate-every-box ul.list-ul li { padding-right: 35px; color: #6f6f6f; float: left; cursor: pointer; margin: 8px 0 20px 0; font-size: 13px; }
.evaluate-body .evaluate-every-box ul.list-ul li:hover { color: #2494f2; }
.evaluate-body .evaluate-every-box ul.list-ul li.active { color: #2494f2; }
.evaluate-body .evaluate-every-box .bjj-list-empty { height: 200px; margin-top: 150px; }
.evaluate-body .evaluate-every-box ul.list-ul li:hover { color: #2494f2; }
.evaluate-body .evaluate-every-box ul.list-ul li.active { color: #2494f2; }
.evaluate-body .evaluate-every-box .bjj-list-empty { height: 200px; margin-top: 150px; }
.evaluate-body .evaluate-every-box .table { border: 1px solid #eceff8; }
.evaluate-body .evaluate-every-box .table .pagination li { margin-right: 10px; }
.evaluate-body .evaluate-every-box .table thead tr,.evaluate-body .evaluate-every-box .table tbody tr { width: 100%; height: 49px; }
.evaluate-body .evaluate-every-box .table thead tr td,.evaluate-body .evaluate-every-box .table tbody tr td { line-height: 38px; border-bottom: 1px solid #eceff8; text-align: right;}
.evaluate-body .evaluate-every-box.site-ranking { padding-bottom: 55px; }
.evaluate-body .evaluate-every-box .table thead tr td.index,.evaluate-body .evaluate-every-box .table tbody tr td.index { text-align: center; }
.evaluate-body .evaluate-every-box .table tbody tr td.index span { color: #2494f2; }
.evaluate-body .evaluate-every-box .table tbody tr td.index span:hover { cursor: pointer; }
.evaluate-every-box .table tbody tr td.channel-active { color: #00CCAF; }
.evaluate-every-box .table tbody tr td.channel-no-active { color: #FFA16E ; }
.evaluate-every-box .table tbody tr td.channel-abnormal { color: #F86574; }
.evaluate-every-box .table-striped>tbody>tr:nth-of-type(odd),.evaluate-every-box .table-striped>tbody>tr:nth-of-type(odd):hover { background: #fff; }
.evaluate-body .evaluate-every-box .num-page { width: auto; float: right; height: 45px; }
.evaluate-body .evaluate-every-box .num-page .total-count { float: left; margin-right: 25px; line-height: 35px; }
.evaluate-body .evaluate-every-box .num-page .page-tion { margin-top: 5px; margin-right: 5px; }
.evaluate-body .evaluate-every-box .num-page button { height: 28px; line-height: 14px; margin-top: 5px; }
.evaluate-index .evaluate-channel { height: 60px; }
.evaluate-index .evaluate-channel p .channels-name { font-size: 21px; margin-right: 24px;}
.evaluate-index .evaluate-channel p .summary-time { color: #a4a4a4; margin-right: 24px; font-size: 13px; }
.evaluate-index .evaluate-channel p .summary-name { color: #a4a4a4; font-size: 13px; }
.evaluate-index .evaluate-channel span strong { font-weight: normal; font-size: 22px; margin-left: 5px; margin-top: 8px;}
.evaluate-index .evaluate-channel .channels-num { margin-right: 24px; }
.evaluate-index .evaluate-detail-index { width: 100%; height: 140px; border: 1px solid #eceff8; border-left: none;  }
.evaluate-index .evaluate-detail-index ul li { width: calc(20% - 1px); height: 140px; border-left: 1px solid #eceff8; padding: 27px 0 22px 23px; position: relative; }
.evaluate-index .evaluate-detail-index ul li p { margin: 0; }
.evaluate-index .evaluate-detail-index ul li p span:first-child { color:  #6f6f6f; }
.evaluate-index .evaluate-detail-index ul li p.tips { position: absolute; display: none; z-index: 10; width: 275px; height: 40px; background: #fff; line-height: 40px; padding-left: 30px; border: 1px solid #eceff8; top: 53px; left: 40px;}
.evaluate-index .evaluate-detail-index ul li p.tips.tsi-tips-p { left: -150px;}
.evaluate-index .evaluate-detail-index ul li p b { font-weight: normal; float: left; margin-right: 5px; color: #a4a4a4;}
.evaluate-index .evaluate-detail-index ul li p i{ width: 10px; height: 11px;margin: 4px 5px 0 0px; float: left; background-image: url(/theme/default/images/trend-marks.png);}
.evaluate-index .evaluate-detail-index ul li p.top i { background-position: 0px -33px; }
.evaluate-index .evaluate-detail-index ul li p.down i { background-position: 10px -33px; }
.evaluate-index .evaluate-detail-index ul li p.top span.trend { color: #F86574; }
.evaluate-index .evaluate-detail-index ul li p.down span.trend { color: #00CCAF; }
.evaluate-index .evaluate-detail-index ul li .explain {display: inline-block; cursor: pointer; width: 14px; height: 14px; padding-left: 3px; color:#a4a4a4; margin-left: 5px; line-height: 12px; border: 1px solid #a4a4a4; border-radius: 50%; font-size: 10px; }
.evaluate-index .evaluate-detail-index ul li .detail-index { font-size: 20px; margin-bottom: 22px;}
.evaluate-body .trend-statistics .echarts-box { width: 100%; height: 300px; clear: both; text-align: center;}

.evaluate-body .trend-statistics .echarts-box { height: 285px; clear: both; text-align: center;}
.pagination>li>a, .pagination>li>span{ width: 24px; height: 28px; line-height: 14px; text-align: left; color: #2494f2; padding: 6px 7px; margin-right: 12px; }
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover { background-color: #2494f2; border-color: #2494f2;  }
