package com.istar.mediabroken.entity.evaluate


class TendencyTop {
    String id
    String evaluateId
    def publishCount
    def multiple
    def psi
    def mii
    def tsi
    def bsi
    Date createTime
    Date updateTime


    TendencyTop() {
        super
    }

    Map<String, Object> toMap() {
        return [
                _id         : id,
                evaluateId  : evaluateId,
                publishCount: publishCount,
                multiple    : multiple,
                psi         : psi,
                mii         : mii,
                tsi         : tsi,
                bsi         : bsi,
                updateTime  : updateTime,
                createTime  : createTime


        ]
    }

    TendencyTop(Map map) {
        id = map._id
        evaluateId = map.evaluateId
        publishCount = map.publishCount
        multiple = map.multiple
        psi = map.psi
        mii = map.mii
        tsi = map.tsi
        bsi = map.bsi
        updateTime = map.updateTime
        createTime = map.createTime
    }
}
